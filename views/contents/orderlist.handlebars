<ol class="breadcrumb">
  <li>控制面板</li>
  <li><a href="/org/orguserlist?id={{id}}">用户列表</a></li>
  <li><a href="/org/orderlist?id={{id}}">订单列表</a></li>
</ol>
<table class="table table-hover">
     <thead>
      <tr>
         <th>快递单号</th>
         <th>类型</th>
         <th>状态</th>
         <th>余单数</th>
         <th>操作</th>
      </tr>
   </thead>
   <tbody>
       {{#each users}}
            <tr>
                <td>{{mobile}}</td>
                <td>{{usertype}}</td>
                <td>{{status}}</td>
                <td>{{count}}</td>
                <td>
                  <a href="/admin/userdetail?id={{_id}}" style="cursor:hand" >详情</a>
                </td>
            </tr>
       {{/each}}

   </tbody>
   

</table>

<nav>
    <ul class="pagination">
        </li>
        {{#each pagesArray}}
        <li><a href="/admin/userlist?p={{p}}">{{p}}</a></li>
        {{/each}}
    </ul>
</nav>

