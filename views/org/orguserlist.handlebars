<table class="table table-hover">
     <thead>
      <tr>
         <th>手机号</th>
         <th>类型</th>
         <th>状态</th>
         <th>余单数</th>
         <th>操作</th>
      </tr>
   </thead>
   <tbody>
       {{#each users}}
            <tr>
                <td>{{mobile}}</td>
                <td>{{usertype}}</td>
                <td>{{status}}</td>
                <td>{{count}}</td>
                <td>
                  <a href="/org/orguserdetail?id={{_id}}" style="cursor:hand" >详情</a>
                </td>
            </tr>
       {{/each}}

   </tbody>
   

</table>

<nav>
    <ul class="pagination">
        </li>
        {{#each pagesArray}}
        <li><a href="/org/orguserdetail?p={{p}}">{{p}}</a></li>
        {{/each}}
    </ul>
</nav>

